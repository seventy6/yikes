{
    "name": "Yikes!",
    "short_name": "Yikes! here to remember the important things in life...",
    "description": "Yikes! is a simple reminder app that appears everytime you open a new browser tab.",
    "version": "1.0.2",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "notifications",
        "tabs"
    ],
    "background": {
      "scripts": [
        "scripts/chrome-background.js"
      ]
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}